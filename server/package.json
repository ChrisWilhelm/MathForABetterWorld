{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:seed": "yarn prisma db seed",
    "prisma:studio": "yarn prisma studio",
    "prisma:dev": "yarn docker:up && dotenv -e .env yarn prisma migrate dev",
    "prisma:prod": "dotenv -e .env yarn prisma migrate dev",
    "prisma:reset": "yarn docker:up && dotenv -e .env yarn prisma migrate reset",
    "prisma:test": "yarn docker:up && dotenv -e .env.test yarn prisma migrate dev",
    "start": "node .",
    "dev": "yarn prisma:dev && nodemon .",
    "prod": "yarn prisma:prod && node .",
    "prettier": "prettier --write '{prisma,src,tests}/**/*.js' --config '.prettierrc.json'",
    "test": "yarn prisma:test && NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "supertest": "^6.3.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
